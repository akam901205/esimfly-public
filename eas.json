{
  "cli": {
    "version": ">= 11.0.2",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium",
        "image": "latest",
        "node": "20.19.4"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "env": {
        "NPM_CONFIG_FUND": "false",
        "NPM_CONFIG_UPDATE_NOTIFIER": "false"
      },
      "cache": {
        "disabled": true
      }
    },
    "development-device": {
      "extends": "development",
      "ios": {
        "simulator": false
      },
      "channel": "development-device"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest",
        "node": "20.19.4"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest",
        "node": "20.19.4"
      },
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_USE_FABRIC": "true"
        }
      },
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "support@esimfly.net",
        "ascAppId": "6686394686",
        "appleTeamId": "TKW74372HP"
      }
    }
  }
}
